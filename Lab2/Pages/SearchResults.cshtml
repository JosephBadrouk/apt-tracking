@page
@model SearchResultsModel
@{
}

<div class="text-center">
    <h1 class="display-4">Search Results</h1>
</div>

<hr />

<div class="w3-container">
    <table class="w3-table-all">
        <tr>
            <th>Tactic</th>
        </tr>
        @{
            foreach (var tactic in SearchModel.Results)
            {
                <tr>
                    <td><a style="color: #0366d6; " href="/Technique?name=@tactic">@tactic</a></td>
                </tr>
            }
        }
    </table>
</div>
